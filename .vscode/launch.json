{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Application Module",
            "type": "debugpy",
            "request": "launch",
            "module": "application",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name": "Attach to Python Functions",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 9091
            },
            "preLaunchTask": "func: host start"
        }
    ]
}